{
  "required": true,
  "$schema": "http://json-schema.org/draft-03/schema",
  "type": "object",
  "title": "Reseller",
  "properties": {
    "id": {
      "required": true,
      "type": "string",
      "description": "API client ID"
    },
    "name": {
      "required": false,
      "type": "string",
      "description": "API client name"
    },
    "activation_date": {
      "required": false,
      "type": "date",
      "description": "Activation Date"
    },
    "commission": {
      "required": false,
      "type": "number",
      "description": "Commission"
    },
    "type": {
      "required": false,
      "type": "string",
      "description": "API client type"
    },
    "status": {
      "required": false,
      "type": "string",
      "description": "Status of the API client"
    }
  }
}
